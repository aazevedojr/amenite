<h2>Amenite</h2>
<div id="map"></div>
<script>
  function initMap() {
    var sf = {lat: 37.7848739, lng: -122.3974857};
    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 15,
      center: sf
    });
    var marker = new google.maps.Marker({
      position: sf,
      map: map
    });
  }
</script>

<script async defer
src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAl1si7nDalYmDANWYv8y1lsry4XkFXEmk&callback=initMap">
</script>


