<!DOCTYPE html>
<html>
  <head>
    <style>
       #map {
        height: 400px;
        width: 50%;
       }
    </style>
  </head>
  <body>
    <h3>Amenity</h3>
    <div id="map"></div>
    <script>
      function initMap() {
        var uluru = {lat: 37.7848739, lng: -122.3974857};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 15,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAl1si7nDalYmDANWYv8y1lsry4XkFXEmk&callback=initMap">
    </script>
  </body>
</html>
